<xml>
  <dynamic-substitution id="c3-piss-medium">
    <eval-subst obj="c1">x^{7}</eval-subst>
    <eval-subst obj="c2">7 x^{6}</eval-subst>
    <eval-subst obj="P">\frac{7}{x}</eval-subst>
    <eval-subst obj="Q">\frac{\mathrm{e}^{x}}{x^{7}}</eval-subst>
  </dynamic-substitution>
  <dynamic-substitution id="if-integrating-factor-cyu-3">
    <eval-subst obj="mu">\mathrm{e}^{x^{3}}</eval-subst>
  </dynamic-substitution>
  <dynamic-substitution id="if-integrating-factor-cyu-4">
    <eval-subst obj="P">\frac{1}{x}</eval-subst>
    <eval-subst obj="Q">2+\frac{1}{x}</eval-subst>
    <eval-subst obj="mu">x</eval-subst>
  </dynamic-substitution>
  <dynamic-substitution id="if-integrating-factor-cyu-5">
    <eval-subst obj="P">2 x</eval-subst>
    <eval-subst obj="Q">2</eval-subst>
    <eval-subst obj="mu">\mathrm{e}^{x^{2}}</eval-subst>
  </dynamic-substitution>
</xml>