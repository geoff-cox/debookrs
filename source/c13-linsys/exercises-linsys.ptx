	<subsection label="linsys-concept-quiz">
		<title><e component="emoji">üí°</e> Conceptual Quiz</title>
		
		<exercises label="linsys-cq">
			<aside>
				<title>Abbreviations</title>
				<tabular>
					<col width="10%" />
