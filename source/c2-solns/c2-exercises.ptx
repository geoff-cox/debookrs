					</evaluate>
					<evaluate name="c2_blank">
						<test correct="yes">
							<mathcmp obj="c2" />
						</test>
					</evaluate>
					<evaluate name="P_blank">
						<test correct="yes">
							<mathcmp obj="P" />
						</test>
					</evaluate>
					<evaluate name="Q_blank">
						<test correct="yes">
							<mathcmp obj="Q" />
						</test>
					</evaluate>
				</evaluation>
		
		</exercise>

