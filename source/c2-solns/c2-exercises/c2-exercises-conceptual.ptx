							<p>
								<me>\vphantom{\frac{d^2R}{dt^2}} (\sin x')y - y^2 = 2 </me>
							</p>
							<p>
								<fillin mode="string" answer="x" />
							</p>
							<p>
								<fillin mode="number" answer="1" />
							</p>
						</sidebyside>

					</p>
				</statement>

