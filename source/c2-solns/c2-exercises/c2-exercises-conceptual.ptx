			<task label="c1-cr-fillin-3-fit"><title> <em>Determine the Dependent Variable &amp; Order</em> </title>
			
				<statement>
					<p>
						<sidebyside widths="5% 50% 16% 16%" margins="5%" valigns="middle middle middle bottom">
							<p>
								<m>\quad</m>
							</p>
							<p>	
								<me>
										{\text{Differential Equation}}
								</me>
							</p>
							<p>
								<md>
									<mrow> \text{Dependent} </mrow>
									<mrow> {\text{Variable}} </mrow>
								</md>
							</p>
							<p>
								<me> {\text{Order}} </me>
