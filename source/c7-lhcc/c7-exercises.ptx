								' ';
							Context()->variables->add(r=>'Real');
							$diff_eqn = "w''' - 7.1w'' + 0.1w = 0";
							$char_eqn = ImplicitEquation("r^3 - 7.1*r^2 + 0.1 = 0");
						</pg-code>

						<statement>
							<p>
								<m>\displaystyle <var name="$diff_eqn" /></m>
							</p>
							<p>
								characteristic equation: <var name="$char_eqn" width="10" />
							</p>
						</statement>
					</webwork>
				</exercise>

				<exercise label="drill-give-characteristic-eqn-3">
					<title><m>2 p'' + 5p' = 0</m></title>
					<webwork>
