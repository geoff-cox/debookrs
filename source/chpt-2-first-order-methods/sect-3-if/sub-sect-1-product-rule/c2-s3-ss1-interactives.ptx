<reading-questions><title> Check your Understanding </title>
		
	<exercise><title>Fill-In, String and Number Answers</title>
		<statement>
			<p>
				Fill in the missing parts of the product rule below.
			</p>
			<p>
				<m>	\ds \frac{d}{dx}\left[x^2 \cos x\right] = x^2 \cdot </m> 
				<var width="5px"/> 
				<m> + </m> 
				<var  width="5px"/> 
				<m> \cdot \cos x </m>
			</p>
		</statement>
		<setup>
			<var>
				<condition string="\s*-\s*sin\s*\(?x\)?">
					<feedback>
						<p>
							YES
						</p>
					</feedback>
				</condition>
				<condition string=".*">
					<feedback>
						<p>
							NO
						</p>
					</feedback>
				</condition>
			</var>
			<var>
				<condition string="\s*2\s*x\s*">
					<feedback>
						<p>
							YES
						</p>
					</feedback>
				</condition>
				<condition string=".*">
					<feedback>
						<p>
							NO
						</p>
					</feedback>
				</condition>
			</var>
		</setup>
	</exercise>

</reading-questions>
