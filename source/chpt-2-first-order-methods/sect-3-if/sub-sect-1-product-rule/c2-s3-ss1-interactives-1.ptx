<reading-questions><title> Check your Understanding </title>
		
	<exercise><title>Fill-In the Blank</title>
		<statement>
			<p>
				Fill in the missing parts of each product rule below.
			</p>
			<p>
				<m>	\ds \frac{d}{dx}\left[x^2 \cos x\right] = </m>
				<m> x^2 \cdot</m> <var width="3px"/> <m>+</m> <var  width="3px"/> <m>\cdot \cos x</m>
			</p>
			<p>
				<m>	\ds \frac{d}{dx}\big[e^{3x} </m> <var width="3px"/> <m>\big] = </m>
				<m> \ds \frac{1}{x} \cdot e^{3x} + </m> <var  width="3px"/> <m> e^{3x} \cdot \ln x </m>
			</p>
		</statement>
		<setup>
			<var>
				<condition string="\s*-\s*sin\s*\(?x\)?\s*">
					<feedback>Correct!</feedback>
				</condition>
				<condition string=".*">
					<feedback> Incorrect, try again.</feedback>
				</condition>
			</var>
			<var>
				<condition string="\s*2\s*x\s*">
					<feedback>Correct!</feedback>
				</condition>
				<condition string=".*">
					<feedback> Incorrect, try again.</feedback>
				</condition>
			</var>
			<var>
				<condition string="\s*ln\s*\(?x\)?\s*">
					<feedback>Correct!</feedback>
				</condition>
				<condition string="\s*ln\s*">
					<feedback>Incorrect, expecting to see the independent variable <m>x</m>.</feedback>
				</condition>
				<condition string=".*log.*">
					<feedback>Incorrect, use the natural log instead.</feedback>
				</condition>
				<condition string=".*">
					<feedback> Incorrect, try again.</feedback>
				</condition>
			</var>
			<var>
				<condition number="3">
					<feedback>Correct!</feedback>
				</condition>
				<condition string=".*">
					<feedback> Incorrect, don't forget the chain rule.</feedback>
				</condition>
			</var>
		</setup>
	</exercise>

</reading-questions>
