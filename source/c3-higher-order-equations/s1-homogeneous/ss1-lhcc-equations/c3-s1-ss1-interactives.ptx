<reading-questions><title> Check your Understanding </title>
  
	<exercise label="matching-dates"><title>Matching</title>
		<statement>
			<p>
				Match each differential equation below with the correct classification.
			</p>
		</statement>
		<feedback>
			<p>
				Hey
			</p>
		</feedback>
		<matches>
			<match order="4">
				<premise> 
					<m>\ds r'' + 3r' + 2r^2 = 0 </m>
				</premise>
				<response>non-L, CC</response>
			</match>
			<match order="3">
				<premise> 
					<m>\ds r'' + 3t r'r = 0 </m>
				</premise>
				<response>non-L, non-CC</response>
			</match>
			<match order="1">
				<premise> 
					<m>\ds r'' + 3r' + 2r = 0 </m>
				</premise>
				<response>linear, homogeneous, constant coefficient</response>
			</match>
			<match order="2">
				<premise> 
					<m>\ds r'' + 3t r' + 2r = 0 </m>
				</premise>
				<response>linear, homogeneous, non-constant coefficient</response>
			</match>
			<match order="6">
				<premise> 
					<m>\ds y''' - 7y = e^x </m>
				</premise>
				<response>linear, non-homogeneous, constant coefficient</response>
			</match>
			<match order="5">
				<premise> 
					<m>\ds t^5\frac{d}{dt} = \sin(3t) </m>
				</premise>
				<response>linear, non-homogeneous, non-constant coefficient</response>
			</match>
		</matches>
	</exercise>

