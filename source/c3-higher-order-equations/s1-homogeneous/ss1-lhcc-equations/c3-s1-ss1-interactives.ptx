	<exercise label="label-equations-1"><title>Matching</title>
		<statement>
			<p>
				Match each differential equation below with the correct classification.
				Key: <term>L</term> = linear, <term>H</term> = homogeneous, <term>CC</term> = constant coefficient
			</p>
		</statement>
		<feedback>
			<p>
				Hey
			</p>
		</feedback>
		<matches>
			<match order="4">
				<premise>non-L, CC</premise>
				<response><m>\ds r'' + 3r' + 2r^2 = 0 </m></response>
			</match>
			<match order="3">
				<premise> non-L, non-CC </premise>
				<response><m>\ds r'' + 3t r'r = 0 </m></response>
			</match>
			<match order="1">
				<premise> L, H, CC </premise>
				<response><m>\ds r'' + 3r' + 2r = 0 </m></response>
			</match>
			<match order="2">
				<premise> L, H, non-CC </premise>
				<response><m>\ds r'' + 3t r' + 2r = 0 </m></response>
			</match>
			<match order="6">
				<premise> L, non-H, CC </premise>
				<response><m>\ds y''' - 7y = e^x </m></response>
			</match>
			<match order="5">
				<premise> L, non-H, non-CC </premise>
				<response><m>\ds t^5\frac{d}{dt} = \sin(3t) </m></response>
			</match>
		</matches>
	</exercise>

