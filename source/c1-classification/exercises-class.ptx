								<cell><area correct="no">	<m>\ t\ </m>	</area></cell>
								<cell/>
								<cell><area correct="yes">	yes	</area></cell>
								<cell><area correct="no">	no	</area></cell>
							</row>
							<row>
								<cell>	(d)	</cell>
