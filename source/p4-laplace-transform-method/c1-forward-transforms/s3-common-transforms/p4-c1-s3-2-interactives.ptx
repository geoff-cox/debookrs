	<exercise label="p4-c1-s3-rq7"><title> <em><m>\ds\lap{t} = </m> ?</em> </title>
		<statement/>
		<choices>
			<choice correct="yes">
				<statement><m>\ds\frac{1}{s^2}</m></statement>
				<feedback>
					<p>
						Correct! The Laplace transform of <m>t</m> is <m>\ds\frac{1}{s^2}</m>.
					</p>
				</feedback>
			</choice>
			<choice>
				<statement><m>\ds\frac{1}{s}</m></statement>
				<feedback>
					<p>
						No, the power of <m>s</m> in the denominator is not <m>1</m>.
					</p>
				</feedback>
			</choice>
			<choice>
				<statement><m>1</m></statement>
				<feedback>
					<p>
						No, the Laplace transform of <m>t</m> is not a constant.
					</p>
				</feedback>
			</choice>
			<choice>
				<statement><m>\ds\frac{2}{s^2}</m></statement>
				<feedback>
					<p>
						No, there should not be a <m>2</m> in the numerator.
					</p>
				</feedback>
			</choice>
		</choices>
	</exercise>

	<exercise label="p4-c1-s3-rq8"><title> <em><m>\ds\lap{t^2} = </m> ?</em> </title>
		<statement/>
		<choices>
			<choice>
				<statement><m>\ds\frac{1}{s^3}</m></statement>
				<feedback>
					<p>
						No, the correct transform is <m>\ds\frac{2}{s^3}</m>.
					</p>
				</feedback>
			</choice>
			<choice correct="yes">
				<statement><m>\ds\frac{2}{s^3}</m></statement>
				<feedback>
					<p>
						Correct! The Laplace transform of <m>t^2</m> is <m>\ds\frac{2}{s^3}</m>.
					</p>
				</feedback>
			</choice>
			<choice>
				<statement><m>\ds\frac{1}{s^2}</m></statement>
				<feedback>
					<p>
						No, the correct transform has a <m>2</m> in the numerator.
					</p>
				</feedback>
			</choice>
			<choice>
				<statement><m>\ds\frac{2}{s^2}</m></statement>
				<feedback>
					<p>
						No, the power of <m>s</m> in the denominator should be <m>3</m>.
					</p>
				</feedback>
			</choice>
		</choices>
	</exercise>	
	
</reading-questions>
