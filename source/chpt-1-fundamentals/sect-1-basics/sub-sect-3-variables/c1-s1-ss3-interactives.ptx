	<exercise label="match-variables"><title> <em>Match the Variables</em> </title>
		<statement>
			<p>
				Identify the dependent and independent variables of the following DE:
				<me> z'' - z' = e^m. </me>
			</p>
		</statement>
		<matches>
			<match order="1">
				<premise><m>z</m></premise>
				<response>Dependent Variable</response>
			</match>
			<match order="2">
				<premise><m>m</m></premise>
				<response>Independent Variable</response>
			</match>
		</matches>
	</exercise>
	
	
