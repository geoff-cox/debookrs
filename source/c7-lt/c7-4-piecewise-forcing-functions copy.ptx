									\def\c{2}	\def\d{4} 	\def\e{5}
									\fill[\RegLFill] (\tmin	, \ymin) rectangle (\c		, \ymax);
									\fill[\RegIFill] (\c	, \ymin) rectangle (\d		, \ymax);
									\fill[\RegRFill] (\e	, \ymin) rectangle (\tmax	, \ymax);
			
									% === tick values ===
									\def\tticks{\c, \d, \e}
			
									% === Axes ===
									\draw[-&gt;] (\tmin	, 0) -- (\tmax	,0);
									\node[font=\small, above] at (\tmax - 0.1	, 0) {$t$};
			
									% === Tick marks/numeric labels ===
									\foreach \t in \tticks {
										\draw[black, very thin, dashed]	(\t,\ymin) -- (\t,\ymax);	% t ticks	
									}
									
									% === Labels ===
									\node[font=\scriptsize, \RegLColor] at ({(\tmin+\c)/2}, 1.0) {Graph};
									\node[font=\scriptsize, \RegLColor] at ({(\tmin+\c)/2}, 0.6) {of\ $g_{1}(t)$};
									\node[font=\scriptsize, \RegLColor] at ({(\tmin+\c)/2}, -0.4) {interval\ 1};
									
									\node[font=\scriptsize, \RegIColor] at ({(\c+\d)/2}, 1.0) {Graph};
									\node[font=\scriptsize, \RegIColor] at ({(\c+\d)/2}, 0.6) {of\ $g_{2}(t)$};
									\node[font=\scriptsize, \RegIColor] at ({(\c+\d)/2}, -0.4) {interval\ 2};

