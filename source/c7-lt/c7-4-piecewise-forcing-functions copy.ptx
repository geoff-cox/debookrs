									\node[font=\scriptsize, \RegRColor] at ({(\e+\tmax)/2}, 1.0) {Graph};
									\node[font=\scriptsize, \RegRColor] at ({(\e+\tmax)/2}, 0.6) {of\ $g_{n}(t)$};
