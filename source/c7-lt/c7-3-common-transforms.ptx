		<assemblage xml:id="L1-L12-transforms"><title> ‚ú≥Ô∏è Laplace Transform Table </title>
			<p>
				Assume <m>F(s) = \lap{f(t)}</m>, <m>a</m>, <m>b</m> are constant, and <m>n=0,1,2,3,\ldots</m>
				<paragraphs xml:id="L1-L12">
					<title> Common Laplace Transforms </title>
						<dl>
							<li>
								<title>L<m>_1</m></title>
								<p>
									<me>\lap{1} = \frac{1}{s}</me>
								</p>
							</li>
							<li>
								<title>L<m>_2</m></title>
								<p>
									<me>\lap{e^{at}} = \frac{1}{s - a}</me>
								</p>
							</li>
							<li>
								<title>L<m>_3</m></title>
								<p>
									<me>\lap{t^n} = \frac{n!}{s^{n+1}}</me>
								</p>
							</li>
							<li>
								<title>L<m>_4</m></title>
								<p>
									<me>\lap{\sin(bt)} = \frac{b}{s^2+b^2}</me>
								</p>
							</li>
							<li>
								<title>L<m>_5</m></title>
								<p>
									<me>\lap{\cos(bt)} = \frac{s}{s^2+b^2}</me>
								</p>
							</li>
							<li>
								<title>L<m>_6</m></title>
								<p>
									<me>\lap{e^{at} t^n} = \frac{n!}{(s-a)^{n+1}}</me>
								</p>
							</li>
							<li>
								<title>L<m>_7</m></title>
								<p>
									<me>\lap{e^{at}\sin(bt)} = \frac{b}{(s-a)^2+b^2}</me>
								</p>
							</li>
							<li>
								<title>L<m>_8</m></title>
								<p>
									<me>\lap{e^{at}\cos(bt)} = \frac{s-a}{(s-a)^2+b^2}</me>
								</p>
							</li>
							<li>
								<title>L<m>_9</m></title>
								<p>
									<me>\lap{f'(t)} = sF(s) - f(0)</me>
								</p>
							</li>
							<li>
								<title>L<m>_{10}</m></title>
								<p>
									<me>\lap{f''(t)} = s^2F(s) - sf(0) - f'(0)</me>
								</p>
							</li>
							<li>
								<title>L<m>_{11}</m></title>
								<p>
									<me>\lap{f'''(t)} = s^3F(s) - s^2f(0) - sf'(0) - f''(0)</me>
								</p>
							</li>
							<li>
								<title>L<m>_{12}</m></title>
								<p>
									<me>\lap{t^n f(t)} = (-1)^n \frac{d^n}{ds^n}F(s)</me>
								</p>
							</li>
						</dl>
				</paragraphs>
				<paragraphs xml:id="L1">
					<title>L<m>_1</m><m>\ds\quad\lap{ 1 } = \frac{1}{s}, \quad s &gt; 0</m></title>
				</paragraphs>
				<paragraphs xml:id="L2">
					<title>L<m>_2</m><m>\ds\quad\lap{ e^{at} } = \frac{1}{s - a}, \quad s &gt;a </m></title>
				</paragraphs>
				<paragraphs xml:id="L3">
					<title>L<m>_3</m><m>\ds\quad\lap{ t^n } = \frac{n!}{s^{n+1}}, \quad s &gt;0, \quad n = 1, 2, 3, \ldots </m></title>
				</paragraphs>
				<paragraphs xml:id="L4">
					<title>L<m>_4</m><m>\ds\quad\lap{ \sin(bt) } = \frac{b}{s^2+b^2}, \quad s &gt;0</m></title>
				</paragraphs>
				<paragraphs xml:id="L5">
					<title>L<m>_5</m><m>\ds\quad\lap{ \cos(bt) } = \frac{s}{s^2+b^2}, \quad s &gt;0</m></title>
				</paragraphs>
				<paragraphs xml:id="L6">
					<title>L<m>_6</m><m>\ds\quad\lap{ e^{at}\ t^n } = \frac{n!}{(s-a)^{n+1}}, \quad s &gt;a, \quad n = 0, 1, 2, 3, \ldots </m></title>
				</paragraphs>
				<paragraphs xml:id="L7">
					<title>L<m>_7</m><m>\ds\quad\lap{ e^{at}\sin(bt) } = \frac{b}{(s-a)^2+b^2}, \quad s &gt;a</m></title>
				</paragraphs>
				<paragraphs xml:id="L8">
					<title>L<m>_8</m><m>\ds\quad\lap{ e^{at}\cos(bt) } = \frac{s-a}{(s-a)^2+b^2}, \quad s &gt;a</m></title>
				</paragraphs>
				<paragraphs>
					<title> Laplace Transform of Derivatives </title>
				</paragraphs>
				<paragraphs xml:id="L9">
